<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <!-- Sky background -->
  <rect width="600" height="400" fill="#3498db" />
  
  <!-- Ground -->
  <rect x="0" y="350" width="600" height="50" fill="#34495e" />
  
  <!-- Buildings -->
  <g>
    <!-- Empire State Building -->
    <rect x="150" y="150" width="50" height="200" fill="#7f8c8d" />
    <rect x="160" y="100" width="30" height="50" fill="#7f8c8d" />
    <rect x="165" y="50" width="20" height="50" fill="#7f8c8d" />
    <rect x="170" y="20" width="10" height="30" fill="#7f8c8d" />
    
    <!-- Windows -->
    <g fill="#f1c40f">
      <rect x="160" y="170" width="5" height="10" />
      <rect x="170" y="170" width="5" height="10" />
      <rect x="180" y="170" width="5" height="10" />
      <rect x="160" y="190" width="5" height="10" />
      <rect x="170" y="190" width="5" height="10" />
      <rect x="180" y="190" width="5" height="10" />
      <rect x="160" y="210" width="5" height="10" />
      <rect x="170" y="210" width="5" height="10" />
      <rect x="180" y="210" width="5" height="10" />
      <rect x="160" y="230" width="5" height="10" />
      <rect x="170" y="230" width="5" height="10" />
      <rect x="180" y="230" width="5" height="10" />
    </g>
    
    <!-- Skyscraper 1 -->
    <rect x="220" y="180" width="40" height="170" fill="#2c3e50" />
    
    <!-- Windows -->
    <g fill="#f1c40f">
      <rect x="225" y="190" width="5" height="10" />
      <rect x="235" y="190" width="5" height="10" />
      <rect x="245" y="190" width="5" height="10" />
      <rect x="225" y="210" width="5" height="10" />
      <rect x="235" y="210" width="5" height="10" />
      <rect x="245" y="210" width="5" height="10" />
      <rect x="225" y="230" width="5" height="10" />
      <rect x="235" y="230" width="5" height="10" />
      <rect x="245" y="230" width="5" height="10" />
    </g>
    
    <!-- Skyscraper 2 -->
    <rect x="280" y="200" width="60" height="150" fill="#16a085" />
    
    <!-- Windows -->
    <g fill="#f1c40f">
      <rect x="290" y="210" width="5" height="10" />
      <rect x="300" y="210" width="5" height="10" />
      <rect x="310" y="210" width="5" height="10" />
      <rect x="320" y="210" width="5" height="10" />
      <rect x="290" y="230" width="5" height="10" />
      <rect x="300" y="230" width="5" height="10" />
      <rect x="310" y="230" width="5" height="10" />
      <rect x="320" y="230" width="5" height="10" />
      <rect x="290" y="250" width="5" height="10" />
      <rect x="300" y="250" width="5" height="10" />
      <rect x="310" y="250" width="5" height="10" />
      <rect x="320" y="250" width="5" height="10" />
    </g>
    
    <!-- Freedom Tower -->
    <polygon points="380,350 400,100 420,350" fill="#bdc3c7" />
    
    <!-- Windows -->
    <g fill="#f1c40f">
      <rect x="390" y="150" width="5" height="10" />
      <rect x="400" y="150" width="5" height="10" />
      <rect x="390" y="170" width="5" height="10" />
      <rect x="400" y="170" width="5" height="10" />
      <rect x="390" y="190" width="5" height="10" />
      <rect x="400" y="190" width="5" height="10" />
      <rect x="390" y="210" width="5" height="10" />
      <rect x="400" y="210" width="5" height="10" />
    </g>
    
    <!-- Statue of Liberty (simplified) -->
    <g>
      <rect x="500" y="200" width="20" height="150" fill="#27ae60" />
      <polygon points="490,200 530,200 510,170" fill="#27ae60" />
      <rect x="505" y="150" width="10" height="20" fill="#f5b041" />
      <rect x="508" y="130" width="4" height="20" fill="#f5b041" />
    </g>
  </g>
  
  <!-- Sun -->
  <circle cx="100" cy="80" r="40" fill="#f39c12" />
  
  <!-- Clouds -->
  <g fill="#ecf0f1">
    <ellipse cx="200" cy="100" rx="50" ry="30" />
    <ellipse cx="230" cy="90" rx="40" ry="25" />
    <ellipse cx="170" cy="90" rx="30" ry="20" />
    
    <ellipse cx="400" cy="150" rx="60" ry="35" />
    <ellipse cx="440" cy="140" rx="50" ry="30" />
    <ellipse cx="370" cy="140" rx="40" ry="25" />
  </g>
