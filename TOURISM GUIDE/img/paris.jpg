<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <!-- Sky background -->
  <rect width="600" height="400" fill="#3498db" />
  
  <!-- Ground -->
  <rect x="0" y="300" width="600" height="100" fill="#2ecc71" />
  
  <!-- Eiffel Tower -->
  <g fill="#34495e">
    <!-- Base -->
    <rect x="275" y="300" width="50" height="10" />
    
    <!-- First level -->
    <polygon points="280,300 320,300 350,250 250,250" />
    
    <!-- Second level -->
    <polygon points="260,250 340,250 360,200 240,200" />
    
    <!-- Third level -->
    <polygon points="250,200 350,200 330,150 270,150" />
    
    <!-- Top spire -->
    <polygon points="280,150 320,150 310,100 290,100" />
    <rect x="295" y="50" width="10" height="50" />
  </g>
  
  <!-- Sun -->
  <circle cx="500" cy="80" r="40" fill="#f39c12" />
  
  <!-- Clouds -->
  <g fill="#ecf0f1">
    <ellipse cx="100" cy="100" rx="50" ry="30" />
    <ellipse cx="130" cy="90" rx="40" ry="25" />
    <ellipse cx="70" cy="90" rx="30" ry="20" />
    
    <ellipse cx="400" cy="150" rx="60" ry="35" />
    <ellipse cx="440" cy="140" rx="50" ry="30" />
    <ellipse cx="370" cy="140" rx="40" ry="25" />
  </g>
</svg>